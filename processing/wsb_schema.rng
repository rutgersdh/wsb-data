                  </group>
               </oneOrMore>
               <oneOrMore>
                  <group>
          
                     <ref name="model.graphicLike"/>
          
          
                     <zeroOrMore>
                        <ref name="model.global"/>
                     </zeroOrMore>
          
                  </group>
               </oneOrMore>
            </choice>
            <zeroOrMore>
               <group>
        
                  <ref name="model.divBottom"/>
        
        
                  <zeroOrMore>
                     <ref name="model.global"/>
                  </zeroOrMore>
        
               </group>
